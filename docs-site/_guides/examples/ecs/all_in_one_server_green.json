{
  "containerDefinitions": [
    {
      "name": "server-green",
      "image": "turbinelabs/all-in-one-server:latest",
      "cpu": 1,
      "memory": 128,
      "memoryReservation": 128,
      "portMappings": [
        {
          "hostPort": 8081,
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "dockerLabels": {
        "version": "green",
        "stage": "prod",
        "tbn-cluster": "all-in-one-server:8081"
      },
      "environment": [
        {
          "name": "TBN_COLOR",
          "value": "83D061"
        }
      ]
    }
  ],
  "family": "server"
}
